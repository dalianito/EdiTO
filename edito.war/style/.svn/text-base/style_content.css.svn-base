/* main page layout */
img{
	border:0px;
}
body table, body input,body select,body textarea{
	font-family: "宋体", "Verdana", "Arial";
	color: #000000;
	font-size: 13px;
}
body{
	background-color:#C5C5C5;
	text-align:center;
	margin:0px auto;
	font-size:13px;
}
.BodyView {
	margin: 0px auto;
	background-color: #ffffff;
    font-family:"Verdana", "Arial","宋体";
	color: #000000;
	text-align:center;
	font-size: 13px;
}
a{ color:#330099;TEXT-DECORATION:none }
a.xh{ color:#330099;TEXT-DECORATION:none }
a:visited{color:#330099;text-decoration:none;}
a:hover{color:#990000;text-decoration:none;}
.header{
	width:960px;
	height:50px;
	background-image:url("../images/mainContent/header.gif");
	background-position:center;
	font-family:"Verdana", "Arial","宋体";
	color:#FFFFFF;
	font-size:13px;
}
.main{
	min-height:650px;
	margin:0px auto;
	background-color:#F7F7F7;
	background-image:url("../images/mainContent/background.gif");
	background-repeat:repeat-x;
	background-position:0 0px;
	padding-left:10px;
	overflow: hidden;
}
.topMenu{
	height:20px;
	margin:2px 10px;
	border:0px solid red;
	background-image:url("../images/mainContent/topmenu.gif");
	text-align:left;
	padding-left:0px;
	padding-top:1px;
}
.nav {
	padding:0;
	margin:0;
}
.nav li.top1 {
	display:block;
	float:left;
	height:30px;
}
.nav li.top2 {
	display:block;
	float:right;
	height:30px;
}
.mainContent{
	width:938px;
	min-height:550px;
	background-image:url("../images/mainContent/module_header.gif");
	background-repeat:no-repeat;
	background-color:#FFFFFF;
	border:1px solid #CCCCCC;
	overflow: hidden;
}
.leftMenu{
	margin: 0;	
	width:150px;
	float:left;
	background-color:#FBFBEC;
	border-right:1px solid  #CCCCCC;
	font-family:"黑体","Verdana";
	color:#333333;
	font-size:13px;
	padding-bottom: 3000px;
	margin-bottom: -3000px;
}
.arrow{
	margin: 0;	
	width:8px;
	min-height:500px;
	float:right;
	background-repeat:repeat-y;
	padding-bottom: 3000px;
	margin-bottom: -3000px;
}
.arrow img{   
	padding-top:240px;
}
.content{
	float:left;
	text-align:left;
	margin:18px 18px;
	padding-bottom: 3000px;
	margin-bottom: -3000px;
}
.footer{
	font-size:12px;
	color:#888888;
	width:380px;
	margin:0px auto;
	font-family:"Verdana","Arial","宋体";
	font-weight:bolder;
}

/* DropMenu */
.DropMenu {
	display:none;  
	position:absolute;
}

/* BaseTree */
.BaseTree {
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	padding: 1px 2px 0px 2px;
	border: 1px solid #7F9DB9;
	cursor: pointer;
}

/* DateTime */
.DateTimeMultiple{
	display:none;
}
.DateTimeQuery{
	display:none;
}
.DateTimeSingle{
	border: 1px solid #7F9DB9;
	cursor: pointer;
	padding:1px 2px 0px 2px;
	font-size: 13px;
	font-family: "宋体", "Verdana", "Arial";
}
.YearInput{
	width: 32px;
	height: 14px;
	font-color:#330066;
	text-align:center;
	font-size: 13px;
	ime-mode: disabled;
}
#DateInput{
	border-style:none;
}
#DateInput table{
	border-collapse:collapse;
	width: 180px;
}
#DateInput th{
	background-color: #d2691e;
	color:#ffffff;
}
#DateInput td{
	cursor: pointer;
	color :	#330066;
}
#DateInputTable td{
	font-weight:bold;
	background-color: #e6e6fa;
}
#DateInput input,select{
	margin: 0px;
}
#DateInput .Selected{
	background-color: #f4a460;
	color: #ffffff;
}
#DateInput .WeekEnd{
	color: #f57070;
}
#DateInput .WeekEndTh{
	background-color: #f57070;
	color:#ffffff;
}
#DateInput .Today{
	background-color: #87cefa;
}

/* table: which has input area */
.InputPanel{
	border-collapse:collapse;
	border-style: solid;
	border-color: #D4DDE6;
	border-width: 1px 1px 0px 0px;
	background-color: #fcfcfc;
	color: #000000;
	margin: 0px 0px 0px 0px;
	padding: 2px 3px;
}
.InputPanel td{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	border-style: solid;
	border-color: #D4DDE6;
	border-width: 0px 0px 1px 1px;
	padding: 3px 0px 3px 5px;
	margin: 0px 0px 0px 0px;
	text-align: left;
	vertical-align: center;
	height:20px; 
}
.InputPanel th{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	background-color: #DFE9f5;
	border-style: solid;
	border-color: #D4DDE6;
	border-width: 0px 0px 1px 1px;
	padding: 3px 0px 1px 5px;
	margin: 0px 0px 0px 0px;
	text-align: center;
	color: #374471;
	height:20px; 
}

/* table : which shows the input information green*/
.InfoShowPanel{
	border-style: solid;
	border-color: #C1D8AB;
	border-width: 1px 1px 0px 0px;
	background-color: #F8FAF5;
	color: #000000;
	margin:auto;
}
.InfoShowPanel tr{
height:25px;
}
.InfoShowPanel td{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	border-style: solid;
	border-color: #C1D8AB;
	border-width: 0px 0px 1px 1px;
	padding: 3px 0px 1px 5px;
	vertical-align: center;
	height:20px; 
}
.InfoShowPanel th{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	background-color: #DDEDCB;
	border-style: solid;
	border-color: #C1D8AB;
	border-width: 0px 0px 1px 1px;
	padding: 3px 0px 1px 5px;
	text-align: center;
	color: #666699;
	height:20px; 
}

/* table : which shows the info with different color*/
.InfoListPanel{
	border-style: solid;
	border-color: #CDCDCD;
	border-width: 1px 1px 0px 0px;
	background-color: #F9F9F9;
	color: #000000;
	margin-left: 0px;
}
.InfoListPanel td{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	border-style: solid;
	border-color: #CDCDCD;
	border-width: 0px 0px 1px 1px;
	padding: 0px 0px 1px 3px;
	vertical-align: center;
	height: 20px;
}
.InfoListPanel th{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	background-color: #DFDFDF;
	border-style: solid;
	border-color: #CDCDCD;
	border-width: 0px 0px 1px 1px;
	padding: 3px 0px 1px 3px;
	text-align: center;
	color: #666666;
	height: 20px;
}
.InfoListPanel_color{
	border-collapse: collapse;
	border-style: solid;
	border-color: #CDCDCD;
	border-width: 1px 1px 0px 0px;
	padding: 2px 3px;
}
.InfoListPanel_color td{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	border-style: solid;
	border-color: #CDCDCD;
	border-width: 0px 0px 1px 1px;
	vertical-align: center;
	height: 20px;
}
.InfoListPanel_color th{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	background-color: #F6F8D3;
	border-style: solid;
	border-color: #CDCDCD;
	border-width: 0px 0px 1px 1px;
	padding: 3px 0px 1px 3px;
	text-align: center;
	color: #666666;
	height: 20px;
}
.tr1{
	text-align: center;
	background-color: #FFFFFF;
}
.tr2{
	text-align: center;
	background-color: #EFEFEF;
}

/* table : which has print area */
.PrintPanel{
	border-collapse:collapse;
	border-style: solid;
	border-color: #000000;
	border-width: 1 1 0 0;
	color: #000000;
	margin-left: 0px;
}
.PrintPanel tr{
	height:26px;
}
.PrintPanel td{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 13px;
	border-style: solid;
	border-width: 0 0 1 1;
	padding: 3 0 3 5;
	vertical-align: center;
}
.PrintPanel th{
	font-family: "宋体", "Verdana", "Arial";
	font-size: 14px;
	border-style: solid;
	border-width: 0 0 1 1;
	padding: 3 0 1 5;
	text-align: center;
}
/* cover */
#coverBg{
	display:none;
	z-index:3;
	position:absolute;
	left:0px;
	top:0px;
	filter:Alpha(Opacity=30);
	/* IE */
	-moz-opacity:0.4; 
	/* Moz + FF */
	opacity: 0.4; 
}
#coverDlg {
	position:absolute;
	width:200px;
	display: none;
	z-index: 5;
}
