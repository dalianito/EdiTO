/* login page layout */
.body_login{
 	background-color:#FFFFFF;
	text-align:center;
	width:780px;
	margin:30px auto;
	height:520px;
}
.content_login{
    width:780px;
	height:520px;
    margin:0px;
	background-image:url("../images/loginEdito/login_background.gif");
	background-repeat:repeat-x; 
    background-position:0 0px;
	border:1px solid #242C3C;
}
.content_logout{
    width:780px;
	height:520px;
    margin:0px;
	background-image:url("../images/loginEdito/logout_background.gif");
	background-repeat:repeat-x; 
    background-position:0 0px;
	border:1px solid #242C3C;
}
.login{
	width:600px;
	font-size:14px;
	font-color:#CCCCCC;
	font-family:"黑体","Verdana", "Arial";
	margin:285px auto 120px auto;
	text-align:right;
}
/* main page layout */
img{
	border:0px;
}
body table, body input,body select,body textarea{
	font-family: "宋体", "Verdana", "Arial";
	color: #000000;
	font-size: 13px;
}
body{
	background-color:#C5C5C5;
	text-align:center;
	width:866px;
	margin:0px auto;
	font-size:13px;
}
a{ color:#330099;TEXT-DECORATION:none }
a.xh{ color:#330099;TEXT-DECORATION:none }
a:visited{color:#330099;text-decoration:none;}
a:hover{color:#990000;text-decoration:none;}
.header{
	width:866px;
	height:70px;
	background-image:url("../images/loginEdito/header.gif");
	background-position:center;
	font-family:"Verdana", "Arial","宋体";
	color:#FFFFFF;
	font-size:13px;
	margin:0px auto;
}
.main{
	width:950px;
	min-height:500px;
	margin:0px auto;
	background-color:#F7F7F7;
	background-image:url("../images/loginEdito/background.gif");
	background-repeat:repeat-x;
	background-position:0 0px;
	padding-left:10px;
	overflow: hidden;
}
.mainContent{
	margin:0px auto;
	width:866px;
	height:487px;
	background-image:url("../images/loginEdito/module_header.gif");
	background-repeat:no-repeat;
	background-color:#023F5E;
	border:0px,0px,0px,0px solid #CCCCCC;
	overflow: hidden;
}
.footer{
	background-image:url("../images/loginEdito/footer.gif");
	font-size:12px;
	color:#888888;
	width:866px;
	height:132px;
	margin:0px auto;
	font-family:"Verdana","Arial","宋体";
	font-weight:bolder;
}
body{
	background-color:#C5C5C5;
	text-align:center;
	width:960px;
	margin:0px auto;
	font-size:13px;
}
