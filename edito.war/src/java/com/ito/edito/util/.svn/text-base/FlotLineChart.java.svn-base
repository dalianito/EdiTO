package com.ito.edito.util;

import java.util.ArrayList;
import java.util.List;

public class FlotLineChart {
    private List<FlotLineChartData> dataArray;
    private List<List<Object>> ticksData;
    
    public FlotLineChart() {
        dataArray = new ArrayList<FlotLineChartData>();
    }
    
    public void addData(FlotLineChartData dataPoint) {
        dataArray.add(dataPoint);
    }

    public List<FlotLineChartData> getDataArray() {
        return dataArray;
    }

    public void setDataArray(List<FlotLineChartData> dataArray) {
        this.dataArray = dataArray;
    }
    
    public List<List<Object>> getTicksData() {
        return ticksData;
    }
    public void setTicksData(List<List<Object>> ticksData) {
        this.ticksData = ticksData;
    }

}
