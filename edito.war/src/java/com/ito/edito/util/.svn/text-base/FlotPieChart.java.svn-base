package com.ito.edito.util;

import java.util.ArrayList;
import java.util.List;

public class FlotPieChart {
    private List<FlotPieChartData> data;
    
    public FlotPieChart() {
        data = new ArrayList<FlotPieChartData>();
    }
    
    public void addData(FlotPieChartData dataPoint) {
        data.add(dataPoint);
    }

    public List<FlotPieChartData> getData() {
        return data;
    }

    public void setData(List<FlotPieChartData> data) {
        this.data = data;
    }

}
