<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(Person).name" display-name="姓名">
			<depend name="required" />
		</field>
		<field name="instance(Person).genderUuid" display-name="性别">
			<depend name="required" />
		</field>
		<field name="instance(Person).cellPhone" display-name="手机">
			<depend name="required" />
		</field>
		<field name="instance(Candidates).post" display-name="应聘岗位">
			<depend name="required" />
		</field>
		<field name="instance(WorkExperienceInfo).sequence" display-name="工作经历  序号">
			<depend name="required" />
		</field>
		<field name="instance(EducateInfo).sequence" display-name="教育背景  序号">
			<depend name="required" />
		</field>
		<field name="instance(Candidates).recruitStateUuid" display-name="应聘状态">
			<depend name="required" />
		</field>
	</range>
</validation-config>
