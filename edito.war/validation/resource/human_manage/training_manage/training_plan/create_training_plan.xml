<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(TrainingPlan).title" display-name="培训主题">
			<depend name="required" />
		</field>
		<field name="instance(TrainingPlan).lecturerUuid" display-name="讲师">
			<depend name="required" />
		</field>
		<field name="instance(TrainingPlan).trainingTime" display-name="培训日期">
			<depend name="required" />
		</field>
		<field name="instance(TrainingPlan).departmentUuid" display-name="组织机构">
			<depend name="required" />
		</field>
		<field name="instance(ReceiverInfo).receiverUuid" display-name="接收人">
			<depend name="required" />
		</field>
	</range>
</validation-config>
