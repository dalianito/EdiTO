<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(Contract).mouldUuid" display-name="合同模板">
			<depend name="required" />
		</field>
		<field name="instance(Contract).contractNo" display-name="合同编号">
			<depend name="required" />
		</field>
		<field name="instance(Contract).signer" display-name="签约者">
			<depend name="required" />
		</field>
		<field name="instance(Contract).expireTime" display-name="到期时间">
			<depend name="required" />
		</field>
		<field name="instance(Contract).content" display-name="合同内容">
			<depend name="required" />
		</field>
	</range>
</validation-config>
