<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="old_password" display-name="用户旧密码">
			<depend name="required" />
		</field>
		<field name="new_password" display-name="用户新密码">
			<depend name="required" />
		</field>
		<field name="rpt_password" display-name="重复新密码">
			<depend name="required" />
		</field>
	</range>
</validation-config>
