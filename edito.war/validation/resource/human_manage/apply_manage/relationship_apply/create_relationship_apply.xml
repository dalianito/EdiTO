<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(Relationship).teacherUuid" display-name="员工姓名">
			<depend name="required" />
		</field>
		<field name="instance(Relationship).originalUuid" display-name="原组织机构">
			<depend name="required" />
		</field>
		<field name="instance(Relationship).transferUuid" display-name="调往组织机构">
			<depend name="required" />
		</field>
		<field name="instance(Relationship).postTime" display-name="离岗时间">
			<depend name="required" />
		</field>
		<field name="instance(Relationship).arrivalTime" display-name="到岗时间">
			<depend name="required" />
		</field>
	</range>
</validation-config>
