<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(Recruitment).departmentUuid" display-name="需求部门">
			<depend name="required" />
		</field>
		<field name="instance(Recruitment).position" display-name="职位名称">
			<depend name="required" />
		</field>
		<field name="instance(Recruitment).rankUuid" display-name="级别">
			<depend name="required" />
		</field>
		<field name="instance(Recruitment).weeks" display-name="希望几周内到岗">
			<depend name="required" />
		</field>
		<field name="instance(Recruitment).specificTime" display-name="希望到岗日期">
			<depend name="required" />
		</field>
	</range>
</validation-config>
