<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(MaterialScrap).materialUuid" display-name="物资">
			<depend name="required" />
		</field>
		<field name="instance(MaterialScrap).amount" display-name="数量">
			<depend name="required" />
		</field>
		<field name="instance(MaterialScrap).departmentUuid" display-name="组织机构">
			<depend name="required" />
		</field>
		<field name="instance(MaterialScrap).remarks" display-name="备注">
			<depend name="required" />
		</field>
	</range>
</validation-config>
