<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(Material).materialNo" display-name="编号">
			<depend name="required" />
		</field>
		<field name="instance(Material).specification" display-name="规格型号">
			<depend name="required" />
		</field>
		<field name="instance(Material).unit" display-name="单位">
			<depend name="required" />
		</field>
		<field name="instance(Material).typeUuid" display-name="类型">
			<depend name="required" />
		</field>
		<field name="instance(Material).minValue" display-name="警戒线">
			<depend name="required" />
			<depend name="integer" />
		</field>
	</range>
</validation-config>
