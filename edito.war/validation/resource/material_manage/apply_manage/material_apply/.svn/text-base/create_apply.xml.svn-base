<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(OfficeApplication).departmentUuid" display-name="组织机构">
			<depend name="required" />
		</field>
		<field name="instance(OfficeApplication).campusUuid" display-name="校区">
			<depend name="required" />
		</field>
		<field name="instance(OfficeApplicationInfo).materialUuid" display-name="物品名称">
			<depend name="required" />
		</field>
		<field name="instance(OfficeApplicationInfo).materialAmount" display-name="申请数量">
			<depend name="required" />
			<depend name="integer" />
		</field>
	</range>
</validation-config>
