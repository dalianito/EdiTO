<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(MaterialAllocate).materialUuid" display-name="物资">
			<depend name="required" />
		</field>
		<field name="instance(MaterialAllocate).amount" display-name="数量">
			<depend name="required" />
		</field>
		<field name="instance(MaterialAllocate).demandSectorUuid" display-name="需求部门">
			<depend name="required" />
		</field>
		<field name="instance(MaterialAllocate).demandCampusUuid" display-name="需求校区">
			<depend name="required" />
		</field>
		<field name="instance(MaterialAllocate).deployedSectorUuid" display-name="调配部门">
			<depend name="required" />
		</field>
		<field name="instance(MaterialAllocate).deployedCampusUuid" display-name="调配校区">
			<depend name="required" />
		</field>
		<field name="instance(MaterialAllocate).remarks" display-name="备注">
			<depend name="required" />
		</field>
	</range>
</validation-config>
