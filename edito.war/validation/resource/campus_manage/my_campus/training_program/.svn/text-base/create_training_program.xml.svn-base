<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(ApplyInfo).curriculum" display-name="课程设置">
			<depend name="required" />
		</field>
		<field name="instance(ApplyInfo).effect" display-name="教学效果">
			<depend name="required" />
		</field>
		<field name="instance(ApplyInfo).situation" display-name="实际情况">
			<depend name="required" />
		</field>
		<field name="instance(ApplyInfo).suggest" display-name="建议">
			<depend name="required" />
		</field>
	</range>
</validation-config>
