<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(LeaveApplication).position" display-name="职务">
			<depend name="required" />
		</field>
		<field name="instance(LeaveApplication).leaveTypeUuid" display-name="请假类别">
			<depend name="required" />
		</field>
		<field name="instance(LeaveApplication).leaveReason" display-name="请假事由">
			<depend name="required" />
		</field>
	</range>
</validation-config>
