<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="instance(Room).classroomName" display-name="教室名">
			<depend name="required" />
		</field>
		<field name="instance(Room).numberOf" display-name="容纳人数">
			<depend name="required" />
			<depend name="integer" />
		</field>
		<field name="instance(Room).campusUuid" display-name="校区">
			<depend name="required" />
		</field>
	</range>
</validation-config>
