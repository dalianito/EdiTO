<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE validation-config SYSTEM "../validation-config.dtd">
<validation-config>
	<range id="formA" show-error="alert" show-type="all">
		<field name="name" display-name="姓名">
			<depend name="required" />
		</field>
		<field name="no" display-name="员工号">
			<depend name="required" />
		</field>
		<field name="instance(Teacher).graduateInstitution" display-name="毕业院校">
			<depend name="required" />
		</field>
		<field name="instance(Teacher).homePhone" display-name="住宅电话">
			<depend name="required" />
		</field>
		<field name="instance(PreTeachingExpInfo).sequence" display-name="过往授课经历 序号">
			<depend name="required" />
		</field>
	</range>
</validation-config>
