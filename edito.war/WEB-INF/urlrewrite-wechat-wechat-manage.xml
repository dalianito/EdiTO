<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<!--微信管理-->
<urlrewrite>
	<!--关联账户-->
	<rule>
		<from>^/whttps/wechat/wechat/manage/wechat_verify_user.whtml_(\S+)$</from>
		<to>/pages/wechat/wechat_manage/wechat_verify_user.jsp?fromUserName=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/wechat/manage/verify_user.whtml$</from>
		<to>/pages/wechat/wechat/manage/verifyUser.action</to>
	</rule>
	<!--取消关联账户-->
	<rule>
		<from>^/whttps/wechat/wechat/manage/wechat_unverify_user.whtml_(\S+)$</from>
		<to>/pages/wechat/wechat_manage/wechat_unverify_user.jsp?fromUserName=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/wechat/manage/unverify_user.whtml$</from>
		<to>/pages/wechat/wechat/manage/unverifyUser.action</to>
	</rule>
</urlrewrite>
