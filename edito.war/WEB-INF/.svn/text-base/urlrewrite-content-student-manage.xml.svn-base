<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<!--学生管理-->
<urlrewrite>
	<!--学生信息-->
	<rule>
		<from>^/pages/content/student/manage/student/info/query_students.html$</from>
		<to>/pages/content/student_manage/student_info/query_students.jsp</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/info/query_student_info.html$</from>
		<to>/pages/content/student/manage/queryStudent.action</to>
	</rule>
	<rule>
		<from>^/https/content/student/manage/student/info/create_student_info.html$</from>
		<to>/pages/content/student_manage/student_info/create_student_info.jsp</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/info/create_student_info.html$</from>
		<to>/pages/content/student/manage/createStudent.action</to>
	</rule>
	<rule>
		<from>^/views/content/student/manage/student/info/forward_view_student_info.html$</from>
		<to>/pages/content/student/manage/forwardViewStudent.action</to>
	</rule>
	<rule>
		<from>^/https/content/student/manage/student/info/forward_edit_student_info.html$</from>
		<to>/pages/content/student/manage/forwardEditStudent.action</to>
	</rule>	

	<!--学生变动-->

	<!--报名-->
	<rule>
		<from>^/pages/content/student/manage/student/enrollment/query_enrollment.html$</from>
		<to>/pages/content/student_manage/student_enrollment/query_enrollment.jsp</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/enrollment/query_student_Info.html$</from>
		<to>/pages/content/student/manage/queryStudentInfo.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/enrollment/query_class_Infos.html$</from>
		<to>/pages/content/student/manage/queryClassInfos.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/enrollment/find_student.html$</from>
		<to>/pages/content/student/manage/queryFindStudent.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/enrollment/find_class.html$</from>
		<to>/pages/content/student/manage/queryFindClass.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/enrollment/create_enrollment.html$</from>
		<to>/pages/content/student/manage/createEnrollment.action</to>
	</rule>
	<rule>
		<from>^/views/content/student/manage/student/info/forward_view_student.html$</from>
		<to>/pages/content/student/manage/forwardViewStudentInfo.action</to>
	</rule>
	
	<rule>
		<from>^/https/content/student/manage/student/enrollment/query_studentstatus.html_(\w+)$</from>
		<to>/pages/content/student_manage/student_enrollment/query_studentstatus.jsp?mark=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/enrollment/query_studentstatus.html$</from>
		<to>/pages/content/student/manage/queryStudentStatus.action</to>
	</rule>
	<rule>
		<from>^/views/content/student/manage/student/enrollment/forward_view_student_status.html$</from>
		<to>/pages/content/student/manage/forwardViewStudentStatus.action</to>
	</rule>
	
	<!--退班-->
	<rule>
		<from>^/pages/content/student/manage/student/quit/query_quit.html$</from>
		<to>/pages/content/student_manage/student_quit/query_quit.jsp</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/quit/find_Tuition.html$</from>
		<to>/pages/content/student/manage/queryFindTuition.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/content/student/manage/student/quit/create_quit.html$</from>
		<to>/pages/content/student/manage/createQuit.action</to>
	</rule>
	<rule>
		<from>^/views/content/student/manage/student/quit/forward_view_student_status.html$</from>
		<to>/pages/content/student/manage/forwardViewStudentQuit.action</to>
	</rule>
</urlrewrite>
