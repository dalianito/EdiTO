<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
	<!--日常工作-->
<urlrewrite>
	<!--工作日志-->
	<rule>
		<from>^/whttps/wechat/common/work/wechat_work_record.whtml_(\S+)$</from>
		<to>/pages/wechat/common/work/getWechatWorkRecord.action?employeeUuid=$1</to>
	</rule>
	<rule>
		<from>^/whttps/wechat/common/work/create_wechat_work_record.whtml_(\d+)$</from>
		<to>/pages/wechat/common_work/wechat_work_record.jsp?uuid=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/get_work_record.whtml$</from>
		<to>/pages/wechat/common/work/getWechatWorkRecord.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/create_wechat_work_record.whtml$</from>
		<to>/pages/wechat/common/work/createWechatWorkRecord.action</to>
	</rule>
	<rule>
		<from>^/whttps/wechat/common/work/edit_wechat_work_record.whtml$</from>
		<to>/pages/wechat/common/work/forwardEditoWorkRecord.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/wechat_query_workRecord.whtml$</from>
		<to>/pages/wechat/common/work/queryWorkRecord.action</to>
	</rule>
	<rule>
		<from>^/views/wechat/common/work/forward_view_workRecord.whtml$</from>
		<to>/pages/wechat/common/work/forwardViewWorkRecord.action</to>
	</rule>

	<!--网络宣传-->
	<rule>
		<from>^/whttps/wechat/common/work/wechat_network_propaganda.whtml_(\S+)$</from>
		<to>/pages/wechat/common/work/getWechatNetworkPropagandaAction.action?Uuid=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/wechat_query_network_propaganda.whtml$</from>
		<to>/pages/wechat/common/work/queryNetworkPropaganda.action</to>
	</rule>
	<rule>
		<from>^/whttps/wechat/common/work/create_wechat_network_propaganda.whtml_(\d+)$</from>
		<to>/pages/wechat/common_work/wechat_network_propaganda.jsp?uuid=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/create_wechat_network_propaganda.whtml$</from>
		<to>/pages/wechat/common/work/createWechatNetworkPropaganda.action</to>
	</rule>
	<rule>
		<from>^/views/wechat/common/work/forward_wechat_view_network.whtml$</from>
		<to>/pages/wechat/common/work/forwardViewNetWork.action</to>
	</rule>
	<rule>
		<from>^/whttps/wechat/common/work/edit_wechat_edit_network.whtml$</from>
		<to>/pages/wechat/common/work/forwardEditNetWork.action</to>
	</rule>

	<!--请假申请-->
	<rule>
		<from>^/whttps/wechat/common/work/wechat_leave_apply.whtml_(\S+)$</from>
		<to>/pages/wechat/common/work/getWechatLeaveApply.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/create_wechat_leave_apply.whtml$</from>
		<to>/pages/wechat/common/work/createWechatLeaveApply.action</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/get_leave_notes.whtml$</from>
		<to>/pages/wechat/common/work/getLeaveNotes.action</to>
	</rule>
	<rule>
		<from>^/whttps/wechat/common/work/wechat_query_leave_apply.whtml_(\d+)$</from>
		<to>/pages/wechat/common_work/wechat_query_leave_apply.jsp?uuid=$1</to>
	</rule>
	<rule>
		<from>^/ajaxs/wechat/common/work/wechat_query_leave_apply.whtml$</from>
		<to>/pages/wechat/common/work/queryLeaveApply.action</to>
	</rule>
	<rule>
		<from>^/views/wechat/common/work/forward_wechat_view_leave_apply.whtml$</from>
		<to>/pages/wechat/common/work/forwardViewLeaveApply.action</to>
	</rule>
</urlrewrite>
