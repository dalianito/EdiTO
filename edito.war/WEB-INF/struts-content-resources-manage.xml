<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<!--教学资源管理-->
<struts>
	<package name="content-resources-manage" extends="struts-default" namespace="/pages/content/resources/manage">
		<action name="createClassroom" class="com.ito.edito.action.create.CreateRoomAction">
		</action>
		<action name="queryClassroomManage" class="com.ito.edito.action.query.QueryRoomManageAction">
		</action>
		<action name="forwardClassroomManage" class="com.ito.edito.action.get.GetRoomManageAction">
			<result>/pages/content/resources_manage/class_manage/view_class_manage.jsp</result>
		</action>
		<action name="forwardEditClassroom" class="com.ito.edito.action.get.GetRoomManageAction">
			<result>/pages/content/resources_manage/class_manage/edito_class_manage.jsp</result>
		</action>
		<action name="removeClassroom" class="com.ito.edito.action.delete.DeleteRoomAction">
		</action>
	</package>
</struts>
